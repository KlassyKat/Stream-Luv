{
  "name": "stream-luv",
  "version": "1.0.0",
  "description": "Application for auto opening your favorite streams and giving support... automagically",
  "main": "bundle.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/KlassyKat/Stream-Luv.git"
  },
  "scripts": {
    "start": "electron .",
    "pack": "electron-builder --dir",
    "dist": "electron-builder"
  },
  "build": {
    "appId": "com.streamluv.tracker",
    "productName": "Stream Luv",
    "win": {
      "target": "nsis",
      "icon": "./buildResources/icon.png",
      "publish": "github"
    }
  },
  "author": "Gabe Alexander",
  "license": "ISC",
  "devDependencies": {
    "browser-sync": "^2.26.13",
    "electron": "^11.2.0"
  },
  "dependencies": {
    "auto-launch": "^5.0.5",
    "axios": "^0.21.1",
    "electron-context-menu": "^2.4.0",
    "electron-window-state": "^5.0.3",
    "node-localstorage": "^2.1.6",
    "puppeteer-core": "^3.3.0",
    "puppeteer-in-electron": "^3.0.3",
    "sortablejs": "^1.13.0"
  }
}
